{
	"name": "salesnested",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "AzureSql_sales",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_ls_datalakegen2_urdata",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "parse1"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "select1"
				},
				{
					"name": "pid"
				},
				{
					"name": "price"
				},
				{
					"name": "qnt"
				},
				{
					"name": "flattenpid"
				},
				{
					"name": "flattenqnt"
				},
				{
					"name": "flattenprice"
				},
				{
					"name": "skeypid"
				},
				{
					"name": "skeyqnt"
				},
				{
					"name": "skeyprice"
				},
				{
					"name": "join1"
				},
				{
					"name": "join2"
				},
				{
					"name": "select2"
				},
				{
					"name": "derivedColumn2"
				},
				{
					"name": "aggregate1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          trid as string,",
				"          cid as string,",
				"          info as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"source1 parse(jinfo = info ? (pid as string[],",
				"          price as integer[],",
				"          qnt as integer[]),",
				"     format: 'json',",
				"     documentForm: 'singleDocument') ~> parse1",
				"parse1 derive(pid = jinfo.pid,",
				"          price = jinfo.price,",
				"          qnt = jinfo.qnt) ~> derivedColumn1",
				"derivedColumn1 select(mapColumn(",
				"          trid,",
				"          cid,",
				"          pid,",
				"          price,",
				"          qnt",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 select(mapColumn(",
				"          trid,",
				"          cid,",
				"          pid",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> pid",
				"select1 select(mapColumn(",
				"          trid,",
				"          cid,",
				"          price",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> price",
				"select1 select(mapColumn(",
				"          trid,",
				"          cid,",
				"          qnt",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> qnt",
				"pid foldDown(unroll(pid),",
				"     mapColumn(",
				"          trid,",
				"          cid,",
				"          pid",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> flattenpid",
				"qnt foldDown(unroll(qnt),",
				"     mapColumn(",
				"          trid,",
				"          cid,",
				"          qnt",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> flattenqnt",
				"price foldDown(unroll(price),",
				"     mapColumn(",
				"          trid,",
				"          cid,",
				"          price",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> flattenprice",
				"flattenpid keyGenerate(output(skey as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> skeypid",
				"flattenqnt keyGenerate(output(skey as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> skeyqnt",
				"flattenprice keyGenerate(output(skey as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> skeyprice",
				"skeypid, skeyqnt join(skeypid@skey == skeyqnt@skey,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"join1, skeyprice join(skeypid@skey == skeyprice@skey,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join2",
				"join2 select(mapColumn(",
				"          trid = flattenpid@trid,",
				"          cid = flattenpid@cid,",
				"          pid,",
				"          qnt,",
				"          price",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2",
				"select2 derive(bill = qnt*price) ~> derivedColumn2",
				"derivedColumn2 aggregate(groupBy(cid),",
				"     totalbill = sum(bill)) ~> aggregate1",
				"aggregate1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['sales_bill.txt'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> sink1"
			]
		}
	}
}